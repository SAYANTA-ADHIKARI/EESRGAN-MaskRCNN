{
    "exp_name": "MaskRCNN-FocalLoss-EqualWeight",
    "name": "RRDB_ESRGANx4",
    "n_gpu": 5,
    "model": "srgan",
    "distortion": "sr",
    "scale": 4,
    "use_tb_logger": true,
    "clip_value": 1,
    "network_G": {
        "which_model_G": "RRDBNet",
        "in_nc": 3,
        "out_nc": 3,
        "nf": 64,
        "nb": 23,
        "filter": "sobel",
        "filter_size": 99,
        "args": {}
    },
    "network_D": {
        "which_model_G": "discriminator_vgg_128",
        "in_nc": 3,
        "nf": 64,
        "args": {}
    },
    "data_loader": {
        "type": "MyDataMaskRCNNDataloader",
        "args": {
            "data_dir": "./satellite_data/new_patch_data/patch_images",
            "meta_data_dir": "./satellite_data/new_patch_data/train_data.json",
            "mask_dir": "./satellite_data/new_patch_data/patch_masks",
            "batch_size": 10,
            "shuffle": false,
            "validation_split": 0.0,
            "num_workers": 1
        }
    },
    "val_data_loader": {
        "type": "MyDataMaskRCNNDataloader",
        "args": {
            "data_dir": "./satellite_data/new_patch_data/patch_images",
            "meta_data_dir": "./satellite_data/new_patch_data/val_data.json",
            "mask_dir": "./satellite_data/new_patch_data/patch_masks",
            "batch_size": 10,
            "shuffle": true,
            "num_workers": 1,
            "training": false
        }
    },
    "optimizer": {
        "type": "SGD",
        "args": {
            "lr_G": 0.0001,
            "weight_decay_G": 0,
            "beta1_G": 0.9,
            "beta2_G": 0.999,
            "lr_D": 0.0001,
            "weight_decay_D": 0,
            "beta1_D": 0.9,
            "beta2_D": 0.999
        }
    },
    "loss": "cross_entropy",
    "metrics": [
        "accuracy"
    ],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "lr_steps": [
                50000,
                100000,
                200000,
                300000
            ],
            "lr_gamma": 0.5,
            "T_period": [
                250000,
                250000,
                250000,
                250000
            ],
            "restarts": [
                250000,
                500000,
                750000
            ],
            "restart_weights": [
                1,
                1,
                1
            ],
            "eta_min": 0.0000001
        }
    },
    "train": {
        "niter": 89800,
        "warmup_iter": -1,
        "pixel_criterion": "l1",
        "pixel_weight": 0.01,
        "feature_criterion": "l1",
        "feature_weight": 1,
        "gan_type": "ragan",
        "gan_weight": 0.001,
        "D_update_ratio": 1,
        "D_init_iters": 0,
        "manual_seed": 10,
        "val_freq": 898,
        "save_dir": "saved_test/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": true,
        "num_of_saved_models": 4,
        "mask_rcnn_loss": {
            "loss_classifier": 1,
            "loss_box_reg": 1,
            "loss_mask": 1,
            "loss_objectness": 1,
            "loss_rpn_box_reg": 1
        }
    },
    "path": {
        "models": "./saved_test/pretrained_models_EESRGAN_FRCNN",
        "FRCNN_model": "./saved_test/pretrained_models_EESRGAN_FRCNN/FRCNN_model_LR_LR_cowc/",
        "pretrain_model_G": "./pretrained_weights/latest_G.pth",
        "pretrain_model_D": "./pretrained_weights/latest_D.pth",
        "pretrain_model_FRCNN": "./pretrained_weights/latest_FRCNN.pth",
        "pretrain_model_FRCNN_LR_LR": "./saved_test/pretrained_models_EESRGAN_FRCNN/FRCNN_model_LR_LR_cowc/0_FRCNN_LR_LR.pth",
        "training_state": "./saved_test/training_state",
        "strict_load": false,
        "resume_state": "~",
        "val_images": "./saved_test/val_images",
        "output_images": "./saved_test/val_images_cars_new",
        "log": "./saved_test/logs",
        "data_dir_Valid": "./DetectionPatches_256x256/DetectionPatches_256x256/Potsdam_ISPRS/val_dir/LR/",
        "data_dir_F_SR": "./saved_test/Final_SR_images_test/",
        "data_dir_SR": "./saved_test/SR_images_test/",
        "data_dir_SR_combined": "./saved_test/combined_SR_images_216000/",
        "data_dir_E_SR_1": "./saved_test/enhanced_SR_images_1/",
        "data_dir_E_SR_2": "./saved_test/enhanced_SR_images_2/",
        "data_dir_E_SR_3": "./saved_test/enhanced_SR_images_3/",
        "data_dir_Bic": "./DetectionPatches_256x256/DetectionPatches_256x256/Potsdam_ISPRS/HR/x4/",
        "data_dir_LR_train": "./DetectionPatches_256x256/DetectionPatches_256x256/Potsdam_ISPRS/train_dir/LR/",
        "data_dir_Bic_valid": "./DetectionPatches_256x256/DetectionPatches_256x256/Potsdam_ISPRS/Bic/x4",
        "Test_Result_LR_LR_COWC": "./saved_test/Test_Result_LR_LR_COWC/",
        "Test_Result_SR": "./saved_test/Test_Result_SR/"
    },
    "logger": {
        "print_freq": 1,
        "save_checkpoint_freq": 4490
    }
}